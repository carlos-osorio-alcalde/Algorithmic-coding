   "source": [
    "class Solution:\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\n",
    "        for idx in range(len(haystack)):\n",
    "            if haystack[idx:idx+len(needle)] == needle:\n",
    "                return idx\n",
    "        \n",
    "        return -1\n"
   ]
